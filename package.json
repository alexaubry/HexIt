{
  "name": "HexIt",
  "main": "./lib/hex-it",
  "version": "1.0.1",
  "description": "Decimal to/from Hexadecimal Converter",
  "keywords": [
    "hex",
    "hexadecimal",
    "decimal",
    "convert"
  ],
  "activationCommands": {
    "atom-workspace": {
      "ctrl-shift-h": "hex-it:hex",
      "ctrl-alt-shift-h": "hex-it:hex-prefix",
      "ctrl-shift-d": "hex-it:decimal"
    }
  },
  "repository": "https://github.com/alexaubry/HexIt",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  }
}
